{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "pnpm install --no-frozen-lockfile && pnpm build",
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}
